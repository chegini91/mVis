<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="mainAnchor" prefHeight="410" prefWidth="410" AnchorPane.bottomAnchor="10.0"
            AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.tugraz.cgv.multiviewva.controllers.MaximizeChartController">
   <children>
      <BorderPane>
         <center>
            <AnchorPane fx:id="maxAnchor" BorderPane.alignment="CENTER" />
         </center>
         <left>
            <Label fx:id="yAxis" rotate="270.0" text="y" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin></Label>
         </left>
         <bottom>
            <Label alignment="TOP_CENTER" text="x" BorderPane.alignment="CENTER" fx:id="xAxis">
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
               <padding>
                  <Insets left="250.0" />
               </padding></Label>
         </bottom></BorderPane>
   </children>
</AnchorPane>
